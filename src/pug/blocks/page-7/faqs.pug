- const FAQ_FILTERS = [{text: 'Все', target: 'all'},  {text: 'Пчелы-Королевы', target: 'QueenBees'},  {text: 'Пчелы-трутни', target: 'DroneBees'}, {text: 'Рабочие пчелы', target: 'WorkerBees'}];

- const QUESTIONS = [{text: 'What is ICO Crypto?', category: 'QueenBees, icq'}, {text: 'What cryptocurrencies can I use to purchase?', category: 'QueenBees'}, {text: 'How can I participate in the ICO Token sale?', category: 'QueenBees, token'}, {text: 'How do I benefit from the ICO Token?', category: 'QueenBees, token'}, {text: 'Question in the category Client?', category: 'DroneBees'}, {text: 'Question in the category Client?', category: 'DroneBees'}, {text: 'Question in the category Legal?', category: 'WorkerBees'}, {text: 'Question in the category Legal?', category: 'WorkerBees'}];

section.faqs
  .faqs__container.container 
    header.faqs__header
      +title('FAQs', 'h2', 'faqs__title')
      .faqs__text Below we’ve provided a bit of ICO, ICO Token, cryptocurrencies, and few others common question answers. If you have any other questions, please get in touch via email.
    
    .faqs__filters.swiper-container.__js_faq-filters
      .swiper-wrapper 
        each item, index in FAQ_FILTERS 
          - let cl = index === 0 ? ' active' : '';
          +button(item.text, 'faqs__filter swiper-slide button--rounded button--outline-green' + cl, '', '')(type="button", data-target= item.target)
    
    .faqs__accordion.accordion
      each item, index in QUESTIONS
        - let actCl = index === 0 ? ' accordion__item--active': '';
        .accordion__item(class= actCl, data-category= item.category)
          button.accordion__item-header(type="button")= item.text
          .accordion__item-content ICO Crypto - is unique platform; that is secure, smart and easy-to-use platform, and completely disrupting the way businesses raise capital.

